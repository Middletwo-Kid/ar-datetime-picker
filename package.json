{
  "name": "ar-datetime-picker",
  "version": "0.0.5",
  "private": false,
  "main": "./lib/index.js",
  "author": "Armouy",
  "license": "MIT",
  "description": "datetime-picker UI base Vue",
  "files": [
    "lib/*",
    "README.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:theme && npm run build:umd && npm run build:components && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:theme": "gulp build --gulpfile ./src/components/theme-chalk/gulpfile.js && cp-cli ./src/components/theme-chalk/lib lib/theme-chalk",
    "build:umd": "webpack --config build/webpack.prod.umd.conf.js",
    "build:components": "webpack --config build/webpack.prod.components.conf.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "webpack-merge": "5.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.1.2",
    "cp-cli": "^2.0.0",
    "css-loader": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "vue-loader": "15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "peerDependencies": {
    "vue": ">= 2.5.22"
  }
}
